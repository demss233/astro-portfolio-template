---
import Layout from "../layouts/Layout.astro";
import Navbar from "../components/Navbar";
---

<style>
  html {
    scroll-behavior: smooth;
  }
  .wavy-underline {
    text-decoration-line: underline;
    text-underline-offset: 15px;
    text-decoration-style: wavy;
    color: slategrey;
    text-decoration-thickness: 3px;
  }
  .wavy-paragraph {
    text-decoration-line: underline;
    text-underline-offset: 4px;
    text-decoration-style: solid;
    color: slategrey;
  }
  .hero__buttons button {
    transition: 0.4s all cubic-bezier(0.165, 0.84, 0.44, 1);
  }
  .hero__buttons button:first-child {
    border: 1px solid white;
  }
  .hero__buttons button:first-child:hover {
    background: none;
    color: white;
  }
  .hero__buttons button:nth-child(2):hover {
    background: white;
    color: black;
  }
  .about-me {
    width: 80%;
  }
  .contact-me {
    width: 80%;
  }
  .cards {
    width: 90%;
    margin-top: 50px;
    display: grid;
    gap: 14px 20px;
    max-width: 1300px;
    grid-template-columns: repeat(3, 1fr);
  }
  .card {
    background-color: rgba(55, 55, 55, 0.1);
    padding: 20px 5px;
    border: 1px solid rgb(47, 46, 46);
    border-radius: 9px;
    max-width: 400px;
    display: flex;
    flex-direction: column;
    gap: 20px;
    box-shadow: 2px 2px 2px 5px rgba(66, 66, 66, 0.1);
  }
  .card-header {
    font-weight: bold;
    font-size: 2rem;
  }
  @media only screen and (max-width: 521px) {
    .hero {
      margin-top: 160px;
    }
  }
  @media only screen and (max-width: 806px) {
    .hero {
      width: 90%;
    }
  }
</style>

